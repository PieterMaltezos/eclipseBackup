package be.pxl.h4.oef2;

import java.util.Scanner;

//author: Pieter Maltezos
public class H4Oef2 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
				
		System.out.println("Geef uw eerste getal in.");
		int getal1 = input.nextInt();
		
		System.out.println("Geef uw tweede getal in");
		int getal2 = input.nextInt();
		
		int kleinsteGetal = 0;
		int grootsteGetal = 0;
		int uitkomst = kleinsteGetal - grootsteGetal;
		
		if (getal1 < getal2) {
			getal1 = kleinsteGetal;
			getal2 = grootsteGetal;
			System.out.println(uitkomst);
		} else { 
			int uitkomst2 = getal1 - getal2;
			System.out.println(uitkomst2);
		}
		input.close();

	}

}
