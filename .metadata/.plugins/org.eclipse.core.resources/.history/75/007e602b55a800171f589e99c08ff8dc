package be.pxl.h5.opdr8;

public class H5Opdr8 {

	public static void main(String[] args) {
		int a = 1;
		int b = 100;
		
		System.out.println("Resultaat is: " + (a += b));
		System.out.println("Resultaat is: " + (a -= b));
		System.out.println("Resultaat is: " + (a *= b));
		System.out.println("Resultaat is: " + (a /= b));
		System.out.println("Resultaat is: " + (a %= b));

	}

}
