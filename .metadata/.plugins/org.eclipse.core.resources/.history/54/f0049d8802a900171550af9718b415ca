package be.pxl.h5.exoef1;

import java.util.Scanner;

//author: Pieter Maltezos
public class H5ExOef1 {

	public static void main(String[] args) {
		//input graden celsius
		System.out.println("Voer uw graden in celsius in.");
		
		Scanner input = new Scanner(System.in);
		double celsius = input.nextDouble();
		
		double geconverteerd = (9 / 5) * celsius + 32;
		
		System.out.println(geconverteerd);
		
		input.close();
	}

}
